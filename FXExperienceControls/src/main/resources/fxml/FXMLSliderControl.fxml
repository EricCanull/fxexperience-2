<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>

<fx:root style="-fx-background-color: #111;" stylesheets="@../styles/slider_controller.css" type="HBox" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane HBox.hgrow="ALWAYS">
         <children>
            <Region fx:id="alpha_region" style="-fx-background-color: white;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
            <Region fx:id="track_container" onMouseDragged="#onTrackDragged" onMousePressed="#onTrackPressed" prefWidth="244.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
            <StackPane fx:id="thumb" maxWidth="-Infinity" onMouseDragged="#onThumbDragged" onMousePressed="#onThumbPressed" prefHeight="30.0" prefWidth="20.0">
               <children>
                  <Region maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="18.0" prefWidth="18.0" style="-fx-background-color: transparent; -fx-border-color: #FDEAEA; -fx-border-radius: 6; -fx-border-insets: 0; -fx-border-width: 3;" />
                  <Region maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="18.0" prefWidth="18.0" style="-fx-background-color: transparent; -fx-border-color: #585858; -fx-border-radius: 6; -fx-border-insets: 0; -fx-border-width: 2;" />
               </children>
            </StackPane>
         </children>
      </AnchorPane>
   </children>
</fx:root>
